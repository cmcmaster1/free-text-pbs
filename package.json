{
  "name": "free-text-pbs",
  "version": "0.1.0",
  "description": "Free-text PBS search (Postgres + Elasticsearch)",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/api/server.ts",
    "start": "node dist/api/server.js",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "migrate": "tsx src/db/migrate.ts",
    "ingest:latest": "tsx src/scripts/ingest_latest.ts",
    "ingest:month": "tsx src/scripts/ingest_month.ts",
    "backfill": "tsx src/scripts/backfill.ts",
    "eval": "tsx src/eval/runEval.ts",
    "test": "npm run typecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmcmaster1/free-text-pbs.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cmcmaster1/free-text-pbs/issues"
  },
  "homepage": "https://github.com/cmcmaster1/free-text-pbs#readme",
  "dependencies": {
    "@elastic/elasticsearch": "^8.15.2",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "undici": "^7.16.0",
    "unzipper": "^0.12.3",
    "yaml": "^2.8.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/papaparse": "^5.5.2",
    "@types/pg": "^8.16.0",
    "@types/unzipper": "^0.10.11",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
